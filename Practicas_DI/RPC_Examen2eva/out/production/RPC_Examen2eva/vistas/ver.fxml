<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controladoras.ControladoraVer">
   <center>
      <GridPane BorderPane.alignment="CENTER" fx:id="panelVer">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints maxHeight="194.0" minHeight="10.0" prefHeight="82.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="318.0" minHeight="10.0" prefHeight="318.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <JFXTextField />
            <TableView fx:id="tabla" prefHeight="282.0" prefWidth="600.0" GridPane.rowIndex="1">
              <columns>
                <TableColumn fx:id="tNombre" prefWidth="75.0" text="Nombre" />
                <TableColumn fx:id="tApellido" prefWidth="75.0" text="Apellido" />
                  <TableColumn fx:id="tEdad" prefWidth="75.0" text="Edad" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </children>
      </GridPane>
   </center>
</BorderPane>
